# Redroid container configuration
# Used by bin/redroid script

# Docker image
REDROID_IMAGE="redroid/redroid:14.0.0_64only-latest"

# ADB port (host side)
REDROID_PORT=5556

# Tablet display settings
REDROID_WIDTH=2560
REDROID_HEIGHT=1600
REDROID_DPI=320

